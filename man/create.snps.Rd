## The function is currently defined as
function (p, n) 
{
    S <- sample(p)
    S <- S[1:n]
    beta <- rep(0, p)
    beta[S] <- rnorm(n)
    maf <- 0.05 + 0.45 * runif(p)
    return(list(maf = maf, beta = beta))
  }
}
